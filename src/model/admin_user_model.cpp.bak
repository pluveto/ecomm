#include <ecomm/model/admin_user_model.hpp>

#include <cstring>
#include <memory>
#include <ctime>

namespace ecomm
{
    namespace model
    {
        // getters

        std::string admin_user_model::role() 
        {
            return this->role;
        }
        bool admin_user_model::role(std::string role)
        {
            if (role.size() > sizeof(this->role) - 1)
            {
                return false;
            }
            auto s = role.c_str();
            strcpy(this->role, s);
            return true;
        }

    }
}